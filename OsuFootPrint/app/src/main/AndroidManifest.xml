<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.sunshine.android.example.com.osufootprint" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogActivity"
            android:label="LogActivity" >
            <intent-filter>
                <action android:name="com.litreily.LogActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Myfootprints"
            android:label="Myfootprints" >
            <intent-filter>
                <action android:name="com.litreily.Myfootprints" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WishlistActivity"
            android:label="WishListActivity" >
            <intent-filter>
                <action android:name="com.litreily.WishlistActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PopActivity"
            android:label="PopActivity" >
            <intent-filter>
                <action android:name="com.litreily.PopActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewAccount"
            android:label="@string/title_activity_new_account" >
            <intent-filter>
                <action android:name="com.litreily.NewAccountActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>
