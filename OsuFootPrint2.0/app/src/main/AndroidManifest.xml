<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.sunshine.android.example.com.osufootprint20" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogActivity"
            android:label="@string/title_activity_log" >
            <intent-filter>
                <action android:name="android.intent.action.LogActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewAccount"
            android:label="@string/title_activity_new_account" >
            <intent-filter>
                <action android:name="android.intent.action.NewAccountActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Myfootprints"
            android:label="@string/title_activity_myfootprints" >
            <intent-filter>
                <action android:name="android.intent.action.MyfootprintActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PopActivity"
            android:label="@string/title_activity_pop" >
            <intent-filter>
                <action android:name="android.intent.action.PopfootprintActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WishlistActivity"
            android:label="@string/title_activity_wishlist" >
            <intent-filter>
                <action android:name="android.intent.action.MywishlistActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
